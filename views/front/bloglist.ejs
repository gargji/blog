<%- include('../front/header.ejs') %>
<section class="blog-list mtb-40">
    <div class="container">
      <h1>Blogs</h1>
      <div class="row">

        <% b.forEach((item)=>{ %>
          <div class="col-md-4">
            <div class="blog-box">
              <img src="  <%= item.image.url %>" alt="blog1" width="350" height="237" />
              <h3>  <%= item.title %></h3>
              <p>by <span>ADMIN</span> on <span> <%= item.createdAt.toLocaleDateString() %></span></p>
              <p class="text-justify">  <%= item.description.slice(0,120) %></p>
              <a href="/blogdetail/<%= item.id %>" class="btn btn-danger">Read more...</a>
            </div>
          </div>
      <%  }) %>
       
